		BoardDao b_dao = new BoardDao();
		ArrayList<BoardVo> notice = b_dao.listAll(100,"%25EA%25B3%25B5%25EC%25A7%2580%25EC%2582%25AC%25ED%2595%25AD",1,"","");
		ArrayList<BoardVo> board = b_dao.listAll();
